{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/404",
      "dest": "/404.html",
      "status": 404
    }
  ]
}