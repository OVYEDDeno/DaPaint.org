{
  "name": "dapaint",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run type-check",
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export": "expo export --platform web",
    "export:web": "npm run export && node scripts/post-export.js",
    "build": "npm run export:web",
    "vercel-build": "expo export --platform web && node scripts/post-export.js",
    "favicon": "node scripts/create-favicon.js",
    "test:feed": "node scripts/test-feed-functionality.js",
    "test:dapaint": "tsx scripts/test-all-dapaint-functions.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
